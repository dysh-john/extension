{
    "update_url": "https://clients2.google.com/service/update2/crx",
    
        "name": "DYSH extension by John",
        "description": "This extension can make more easy to see",
        "version": "0.1.0",
        "manifest_version": 3
        ,
        "content_scripts": [
            {
                "matches": [ "<all_urls>" ],
                "js": [ "script.js" ]
            }
        ],
        "icons": {
            "16": "logo.png",
            "32": "logo.png",
            "48": "logo.png",
            "128": "logo.png"
        },
        "background": {
            "service_worker": "background.js"
        },
        "action": {
            "default_icon": "logo.png"
         },
         "permissions": ["activeTab"]
}